{
  "name": "telegram-gpt-bot",
  "version": "2.0.0",
  "description": "Telegram bot powered by various large language models.",
  "main": "src/hennos.js",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "npm-run-all clean prisma:generate lint tsc:emit",
    "validate": "npm-run-all lint tsc:noemit",
    "tsc:emit": "tsc",
    "tsc:noemit": "tsc --noEmit",
    "tsc:noemit:watch": "tsc --noEmit --watch",
    "lint": "eslint --fix .",
    "start": "npm run migrate:deploy && node build/hennos.js",
    "start:cli": "node build/client/cli.js",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset",
    "migrate:new": "prisma migrate dev --create-only",
    "prisma:generate": "prisma generate",
    "qdrant": "docker run -p 6333:6333 docker.io/qdrant/qdrant",
    "dev": "npm-run-all build start"
  },
  "author": "Mark Repka",
  "license": "MIT",
  "engines": {
    "node": ">=24.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.59.0",
    "@axiomhq/pino": "^1.3.1",
    "@mozilla/readability": "^0.6.0",
    "@prisma/client": "^5.20.0",
    "@temporalio/client": "^1.14.1",
    "@temporalio/worker": "^1.14.1",
    "@temporalio/workflow": "^1.14.1",
    "axios": "^1.7.7",
    "dotenv": "^16.4.7",
    "jsdom": "^26.1.0",
    "ollama": "^0.5.18",
    "openai": "^5.12.2",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "puppeteer": "^24.16.0",
    "sqlite3": "^5.1.7",
    "tiktoken": "^1.0.21"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^8",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.20.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}