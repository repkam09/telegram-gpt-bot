    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "runtimeArgs": ["--preserve-symlinks"],
            "request": "launch",
            "name": "Launch Hennos",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "tsc",
            "program": "${workspaceFolder}/build/hennos.js",
            "console": "integratedTerminal",
            "sourceMaps": true
        },
        {
            "type": "node",
            "runtimeArgs": ["--preserve-symlinks"],
            "request": "launch",
            "name": "Launch Documents",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "tsc",
            "program": "${workspaceFolder}/build/scripts/documents.js",
            "sourceMaps": true,
            "console": "integratedTerminal"
        },
        {
            "type": "node",
            "runtimeArgs": ["--preserve-symlinks"],
            "request": "launch",
            "name": "Launch Daily Report",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "preLaunchTask": "tsc",
            "program": "${workspaceFolder}/build/scripts/daily-report.js",
            "sourceMaps": true,
            "console": "integratedTerminal"
        }
    ]
}